#!${${PROJECT_NAME}_VENV_DEVEL_DIR}/bin/python
import re
import sys

from setproctitle import setproctitle

PREFIX = "__name:="

for arg in sys.argv:
    if arg.startswith(PREFIX):
        node_name = arg[len(PREFIX):]
        setproctitle(' '.join([node_name] + sys.argv[1:]))
        break

exec(open("${program_path}").read())
